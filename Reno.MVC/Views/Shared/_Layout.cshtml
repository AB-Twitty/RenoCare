    <script>
        function clearFormElements(class_name) {
            $("." + class_name).find(':input').each(function () {
                switch (this.type) {
                    case 'password':
                    case 'text':
                    case 'textarea':
                    case 'file':
                    case 'select-one':
                        $(this).val('').trigger('change');
                        break;
                    case 'select-multiple':
                        $(this).val('').trigger('change');
                        break;
                    case 'date':
                    case 'number':
                    case 'tel':
                    case 'email':
                        $(this).val('');
                        break;
                    case 'checkbox':
                        this.checked = false;
                        $(this).removeAttr('checked').iCheck('update');
                        var classes = $(this).parent().closest('div');
                        $(classes).removeClass('checked ');
                        break;
                    case 'radio':
                        this.checked = false;
                        $(this).iCheck('uncheck').iCheck('update');
                        break;
                }
            });
        }
